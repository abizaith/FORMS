/*Query to assign location to each patient in DB*/
/*This will be applied in each site*/
/*Where:
         463: location of Kibagabaga:
          7: Attribute type= Health Facility
          1: Creator_Id
          '2018-03-27 15:25:49'=created dtae
	      0= voided =0
       uuid() from patient = to generate uuid to each patient
*/
use openmrs;
show tables;
select * from person_attribute where date_created='2018-03-27 15:25:49';
select * from person_attribute_type where person_attribute_type_id in ('7');

use openmrs;
insert into person_attribute (person_id,value,person_attribute_type_id,creator,date_created,voided,uuid) select patient_id,463,7,1,'2018-03-27 15:25:49',0, uuid() from patient where voided=0;
commit;

