/*UPDATE USER_NAME AND PASSWORD*/
/* update #uuids/names and username* on deamon and admin users
( NB: update should be done on users, person and person_name tables)*/

/*(generate uuid for daemonm and admin  to be used in script bellow) */
SELECT UUID();

/*Then run after replacing the uuid*/
UPDATE `billing`.`users` SET `username`='adminmigration', `uuid`='dcdcf915-ae0d-1036-959c-7250392f42bd' WHERE `user_id`='1';
UPDATE `billing`.`users` SET `username`='daemonmigration', `uuid`='86e86d9c-ae11-1036-959c-7250392f42bd' WHERE `user_id`='281';
UPDATE `billing`.`person_name` SET `given_name`='daemonmigration', `uuid`='587c2a8f-ae10-1036-959c-7250392f42bd' WHERE `person_name_id`='123029';
UPDATE `billing`.`person_name` SET `given_name`='Supermigration', `uuid`='229dee9b-ae11-1036-959c-7250392f42bd' WHERE `person_name_id`='1';

/*UPDATE BILLING TABLES*/
/*Where billing is the Database name
      119751: The last number of persons in person table
      281:    The last number of users in users table
NB: users and person tables should be compared in both Billing and HIV instance and we have to consider where there is  the max number
*/

/*update the system_id*/
update billing.users set system_id=concat(user_id,"-",username);
commit;
/*alter the users table and create a new columns on user_id_temp with a field INT(11)*/
ALTER TABLE billing.users  ADD user_id_temp INT(11) AFTER user_id;
commit;
/*create new user_ids in user_temp created   ref to NB above*/
update billing.users set user_id_temp=281+user_id;
commit;
/*alter the users table and create a new columns on person_id_temp with a field INT(11)*/
ALTER TABLE billing.person add person_id_temp INT(11) AFTER person_id;
commit;
/*create new person_ids in person_temp created   ref to NB above*/
update billing.person set person_id_temp=119751+person_id;
commit;
/*update users.creator, users.changed_by, users.retired_by, user_role.user_id, patient.patient_id*/
set foreign_key_checks=0;
update users Set users.creator=281+creator where creator is not null;
update users Set users.changed_by=281+changed_by where changed_by is not null;
update users Set users.retired_by=281+retired_by  where retired_by is not null;
update user_role Set user_role.user_id=281+user_id;
update patient Set patient.patient_id=119751+patient_id;
set foreign_key_checks=1;
/*Compare HIV and Billing attributes_type_id and update if required*/
set foreign_key_checks=0;
update billing.person_attribute set person_attribute_type_id=16 where person_attribute_type_id=10;
set foreign_key_checks=1;
commit;
/*update all tables in Billing affected by user_ids and preson_ids*/
set foreign_key_checks=0;
update moh_bill_beneficiary Set moh_bill_beneficiary.retired_by=281+retired_by where retired_by is not null;
