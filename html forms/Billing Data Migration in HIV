/*
Backup billing tables, role, privilegies, role_role,
Billing global property,
*/
SELECT * FROM kibagabaga_billing.global_property where property like '%bill%';
mysqldump -u root -p --default-character-set=utf8 privilege role_privilege role role_role > billing_users_data.sql
mysqldump -u root -p --default-character-set=utf8 global_property > global_property_data.sql


/*UPDATING HIV*/
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
/*B. STEPS TO BE DONE ON HIV DATABASES*/
/*
1st step: insert INTO hiv instance billing data    >>>>>>  openmrs_moh_hiv_test.person, openmrs_moh_hiv_test.person_address,
openmrs_moh_hiv_test.person_attribute, openmrs_moh_hiv_test.person_name, openmrs_moh_hiv_test.patient,
openmrs_moh_hiv_test.patient_identifier, openmrs_moh_hiv_test.users, openmrs_moh_hiv_test.user_role,
*/
set foreign_key_checks=0;

insert into openmrs.person
(person_id,gender,birthdate,birthdate_estimated,dead,death_date,cause_of_death,creator,date_created,changed_by,date_changed,voided,voided_by,date_voided,void_reason,uuid)
select person_id_temp,gender,birthdate,birthdate_estimated,dead,death_date,cause_of_death,creator,date_created,changed_by,date_changed,voided,voided_by,date_voided,void_reason,uuid
from billing.person;

insert into openmrs.person_address
(person_id,preferred,address1,address2,city_village,state_province,postal_code,country,latitude,longitude,creator,date_created,voided,voided_by,date_voided,void_reason,county_district,address3,address6,address5,address4,uuid,date_changed,changed_by,start_date,end_date)
select person_id,preferred,address1,address2,city_village,state_province,postal_code,country,latitude,longitude,creator,date_created,voided,voided_by,date_voided,void_reason,county_district,address3,address6,address5,address4,uuid,date_changed,changed_by,start_date,end_date
from billing.person_address;

insert into openmrs.person_attribute
(person_id,value,person_attribute_type_id,creator,date_created,changed_by,date_changed,voided,voided_by,date_voided,void_reason,uuid)
select person_id,value,person_attribute_type_id,creator,date_created,changed_by,date_changed,voided,voided_by,date_voided,void_reason,uuid
from billing.person_attribute;

insert into openmrs.person_name
(preferred,person_id,prefix,given_name,middle_name,family_name_prefix,family_name,family_name2,family_name_suffix,degree,creator,date_created,voided,voided_by,date_voided,void_reason,changed_by,date_changed,uuid)
select preferred,person_id,prefix,given_name,middle_name,family_name_prefix,family_name,family_name2,family_name_suffix,degree,creator,date_created,voided,voided_by,date_voided,void_reason,changed_by,date_changed,uuid
from billing.person_name;

insert into openmrs.patient
(patient_id,tribe,creator,date_created,changed_by,date_changed,voided,voided_by,date_voided,void_reason)
select patient_id,tribe,creator,date_created,changed_by,date_changed,voided,voided_by,date_voided,void_reason
from billing.patient;

insert into openmrs.patient_identifier
(patient_id,identifier,identifier_type,preferred,location_id,creator,date_created,voided,voided_by,date_voided,void_reason,uuid,date_changed,changed_by)
select patient_id,identifier,identifier_type,preferred,location_id,creator,date_created,voided,voided_by,date_voided,void_reason,uuid,date_changed,changed_by
from billing.patient_identifier;

insert into openmrs.users
(user_id,system_id,username,password,salt,secret_question,secret_answer,creator,date_created,changed_by,date_changed,person_id,retired,retired_by,date_retired,retire_reason,uuid)
select user_id_temp,system_id,username,password,salt,secret_question,secret_answer,creator,date_created,changed_by,date_changed,person_id,retired,retired_by,date_retired,retire_reason,uuid
from billing.users;

insert into openmrs.user_role
(user_id,role)
select user_id,role
from billing.user_role;

set foreign_key_checks=1;
commit;
